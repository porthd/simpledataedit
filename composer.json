{
    "name": "porthd/simpledataedit",
    "type": "typo3-cms-extension",
    "description": "Simple frontend-editing with TYPO3",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.mobger.de/ns-simpledataedit-typo3-extension/show/desk/",
    "keywords": [
        "TYPO3 CMS",
        "Frontend",
        "editing",
        "Feedit"
    ],
    "authors": [
        {
            "name": "Dr. Dieter Porth",
            "homepage": "https://www.mobger.de/",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^10.1",
        "typo3/cms-extbase": "^10.1",
        "typo3/cms-install": "^10.1"
    },
    "replace": {
        "porthd/simpledataedit": "self.version"
    },
    "require-dev": {

    },
    "config": {
        "bin-dir": ".Build/bin",
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "simpledataedit",
            "web-dir": ".build",
            "cms-package-dir": "{$vendor-dir}/typo3/cms"
        }
    },
    "autoload": {
        "psr-4": {
            "Porthd\\Simpledataedit\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Porthd\\Simpledataedit\\Tests\\": "Tests/"
        }
    }
}
